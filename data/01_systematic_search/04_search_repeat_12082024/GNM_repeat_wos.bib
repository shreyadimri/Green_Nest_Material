@article{ WOS:000813058300001,
Author = {Ramirez, Ismael E. and Causton, Charlotte E. and Gutierrez, George A.
   and Mosquera, Denis A. and Piedrahita, Paolo and Heimpel, George E.},
Title = {Specificity within bird-parasite-parasitoid food webs: A novel approach
   for evaluating potential biological control agents of the avian vampire
   fly},
Journal = {JOURNAL OF APPLIED ECOLOGY},
Year = {2022},
Volume = {59},
Number = {8},
Pages = {2189-2198},
Month = {AUG},
Abstract = {Quantitative food web analyses can provide insights into the specificity
   of consumers such as herbivores, parasites and parasitoids.
   Understanding such patterns can be useful in forecasting the potential
   benefits and risks of biological control agents being considered for
   introduction against invasive species. The avian vampire fly, Philornis
   downsi (Diptera: Muscidae), is a neotropical bird parasite that is
   invasive in the Galapagos Islands, where it is causing substantial
   mortality of endemic bird species. We used a novel in-field experimental
   food web approach within the native range of P. downsi in Ecuador to
   test the hypotheses that pupal parasitoids known to attack P. downsi
   specialize on members of the genus Philornis, which occur only in bird
   nests. We deployed pupae of non-Philornis fly species adjacent to bird
   nests to assess specificity of the parasitoids and used two indices to
   assess specificity: Resource Range (RR), which evaluates the breadth of
   host use, and Pair Difference Index (PDI), which evaluates interaction
   strength. The results revealed very strong compartmentalization within
   the guild of pupal fly parasitoids, with four species attacking only
   Philornis spp. Both specificity indices indicated significant levels of
   specificity towards the genus Philornis for two of these species: Conura
   annulifera and Trichopria sp. novus. We also assessed the specificity of
   the two dominant Philornis species attacking 11 bird species and
   preference of the two dominant parasitoid species for bird species.
   Although there was some significant preference for particular bird
   species by the Philornis spp., there was no indication that this drove
   specificity patterns by the parasitoids. Policy implications. Our
   results confirm previous laboratory studies indicating specificity by C.
   annulifera and support the hypothesis that this species would produce
   few, if any, nontarget impacts if released into Galapagos to suppress
   populations of the P. downsi. These results can inform an environmental
   risk assessment framework to guide governmental agencies in deliberation
   on potential field releases of parasitoids in the Galapagos Islands.},
Type = {Article},
Affiliation = {Ramirez, IE (Corresponding Author), Univ Minnesota, Dept Entomol, St Paul, MN 55108 USA.
   Ramirez, Ismael E.; Heimpel, George E., Univ Minnesota, Dept Entomol, St Paul, MN 55108 USA.
   Causton, Charlotte E.; Mosquera, Denis A., Charles Darwin Fdn, Charles Darwin Res Stn, Santa Cruz, Ecuador.
   Gutierrez, George A.; Piedrahita, Paolo, Escuela Super Politecn Litoral, Fac Ciencias Vida, Guayaquil, Ecuador.},
DOI = {10.1111/1365-2664.14235},
EarlyAccessDate = {JUN 2022},
Author-Email = {ramir238@umn.edu},
Times-Cited = {3},
Unique-ID = {WOS:000813058300001},
DA = {2024-08-12},
}

@article{ WOS:000817740800001,
Author = {Hou, Guoqiang and Fu, Tingjun and Li, Xin and Ma, Qian and Li, Zhong},
Title = {Creation of silanol nests on HZSM-5 catalyst to boost the alkylation of
   toluene with methanol for PX synthesis},
Journal = {APPLIED CATALYSIS A-GENERAL},
Year = {2022},
Volume = {642},
Month = {JUL 25},
Abstract = {HZSM-5 catalysts with abundant silanol nests were fabricated by removing
   framework Al, which effectively promoted the selective alkylation of
   toluene with methanol by inhibiting the deep alkylation of aromatics to
   heavy aromatics and the conversion of methanol to light hydrocarbons.
   Correspondingly, the alkylation efficiency reached 65.3\% at a toluene
   conversion of 31.3\% over dealuminated HZSM-5, higher than the 55.2\%
   efficiency and 28.0\% conversion of parent zeolites, respectively. In
   situ DRIFTS and single-factor experimental clarified that toluene
   adsorption was enhanced and methanol to hydrocarbons was inhibited with
   the presence of silanol nests. Additionally, by increasing the crystal
   size of HZSM-5 with silanol nests from 2 to 5 mu m, para-xylene
   selectivity was increased from 36.1\% to 65.5\% because of the enhanced
   diffusion barrier. The finding of the positive effect of the silanol
   nests on toluene alkylation can provide a solid fundamental basis for
   the rational design of catalysts.},
Type = {Article},
