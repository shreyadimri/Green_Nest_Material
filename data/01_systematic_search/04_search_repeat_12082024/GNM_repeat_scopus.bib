@ARTICLE{Lacroux2022,
	author = {Lacroux, Camille and Pouydebat, Emmanuelle and Rossignol, Marie and Durand, Sophie and Aleeje, Alfred and Asalu, Edward and Chandre, Fabrice and Krief, Sabrina},
	title = {Repellent activity against Anopheles gambiae of the leaves of nesting trees in the Sebitoli chimpanzee community of Kibale National Park, Uganda},
	year = {2022},
	journal = {Malaria Journal},
	volume = {21},
