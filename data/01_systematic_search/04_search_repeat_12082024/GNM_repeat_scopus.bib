	author = {Esteves, Marta and Abreu, Ricardo and Fernandes, Hugo and Serra-Almeida, Catarina and Martins, Patrícia A.T. and Barão, Marta and Cristóvão, Ana Clara and Saraiva, Cláudia and Ferreira, Raquel and Ferreira, Lino and Bernardino, Liliana},
