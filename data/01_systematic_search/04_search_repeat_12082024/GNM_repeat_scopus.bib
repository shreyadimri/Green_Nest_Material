	source = {Scopus},
	note = {Cited by: 0; All Open Access, Gold Open Access}
}

@ARTICLE{Scott-Baumann20221186,
	author = {Scott-Baumann, James F. and Morgan, Eric R. and Cogan, Tristan A.},
	title = {Effects of nest-box environment on fledgling success rate and pathogen load},
	year = {2022},
	journal = {Parasitology},
	volume = {149},
